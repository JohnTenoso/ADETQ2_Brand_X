@model ADETQ2_Brand_X.Models.list

<div class="panel panel-success">
    <div class="panel-heading text-center"><h4>Create Your Account</h4></div>
    <div class="panel-body">

    <form class="p-1" asp-action="privacy" method="post">
        <div asp-validation-summary="All"></div>
        <div class="form-group">
            <label asp-for="studentname">Student Name</label>
            <input class="form-control" asp-for="studentname" />
        </div>
        <div class="form-group">
            <label asp-for="studentno">Student No.</label>
            <input class="form-control" asp-for="studentno" />
        </div>
        <div class="form-group">
            <label asp-for="age">Age</label>
            <input class="form-control" asp-for="age" />
        </div>
        <div class="form-group">
            <label asp-for="address">Address</label>
            <input class="form-control" asp-for="address" />
        </div>
        <div class="text-center">
        <button type="submit">Sign up </button>
         </div>
    </form>
    </div>
</div>
